   "source": [
    "# euler_identity.ipynb\n",
    "# Cell 1\n",
    "\n",
    "import numpy as np\n",
    "from IPython.display import Math\n",
    "\n",
    "z = complex(0, np.pi)\n",
    "ez = 0\n",
    "\n",
    "for p in range(0, 20):\n",
    "    ez += np.power(z, p) / np.math.factorial(p)\n",
    "\n",
    "display(Math(rf\"\\Large e^{{{z:.6f}}} = {ez:.6f}\"))"
   ]
  }
